{
  "name": "oppivelvollisuus-frontend",
  "version": "0.0.1",
  "description": "Frontend for oppivelvollisuus",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 .",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.1",
    "@fortawesome/free-regular-svg-icons": "6.7.1",
    "@fortawesome/free-solid-svg-icons": "6.7.1",
    "@fortawesome/react-fontawesome": "0.2.2",
    "autosize": "^6.0.1",
    "classnames": "^2.5.1",
    "core-js": "^3.39.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "polished": "^4.3.1",
    "react": "^19.0.0",
    "react-csv": "^2.2.2",
    "react-dom": "^19.0.0",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/compat": "^1.2.3",
    "@types/autosize": "^4.0.3",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.7",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^19.0.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "axios": "^1.7.7",
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-expressions": "^1.3.2",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "express": "^4.21.1",
    "express-http-proxy": "^2.1.1",
    "postcss": "^8.4.47",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.1",
    "react-router": "^7.0.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.16.0",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.0.1"
}
