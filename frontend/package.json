{
  "name": "oppivelvollisuus-frontend",
  "version": "0.0.1",
  "description": "Frontend for oppivelvollisuus",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "dev": "concurrently -n tsc,esbuild -c blue,green 'yarn type-check:watch' 'yarn build:serve'",
    "lint": "eslint --max-warnings 0 .",
    "type-check": "tsc --build --force .",
    "type-check:watch": "yarn type-check --watch --preserveWatchOutput",
    "build": "node build.js",
    "build:dev": "node build.js --dev",
    "build:serve": "node build.js --dev --watch --serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.0",
    "autosize": "6.0.1",
    "classnames": "2.5.1",
    "core-js": "3.47.0",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "polished": "4.3.1",
    "react": "19.2.0",
    "react-csv": "2.2.2",
    "react-dom": "19.2.0",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@eslint/compat": "2.0.0",
    "@types/autosize": "4.0.3",
    "@types/lodash": "4.17.21",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-csv": "1.1.10",
    "@types/react-dom": "19.2.3",
    "@types/styled-components": "5.1.36",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "axios": "1.13.2",
    "concurrently": "9.2.1",
    "esbuild": "0.27.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-expressions": "1.3.2",
    "eslint-plugin-lodash": "8.0.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "express": "5.2.1",
    "express-http-proxy": "2.1.2",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.4.0",
    "prettier": "3.7.4",
    "react-router": "7.10.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": true
    }
  },
  "packageManager": "yarn@4.12.0"
}
